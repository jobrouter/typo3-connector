{
	"name": "brotkrueml/typo3-jobrouter-connector",
	"type": "typo3-cms-extension",
	"description": "Define connections from TYPO3 to JobRouter® digitisation platform",
	"keywords": [
		"TYPO3 CMS",
		"TYPO3 extension",
		"JobRouter"
	],
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Chris Müller",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.3",
		"ext-json": "*",
		"ext-sodium": "*",
		"brotkrueml/jobrouter-client": "^1.1",
		"typo3/cms-backend": "^10.4.11 || ^11.5",
		"typo3/cms-core": "^10.4.11 || ^11.5",
		"typo3/cms-extbase": "^10.4.11 || ^11.5",
		"typo3/cms-fluid": "^10.4.11 || ^11.5",
		"typo3/cms-frontend": "^10.4.11 || ^11.5"
	},
	"require-dev": {
		"brotkrueml/coding-standards": "~1.1.0",
		"captainhook/captainhook": "^5.10",
		"clue/phar-composer": "^1.2",
		"codeception/codeception": "^4.1",
		"codeception/module-asserts": "^1.3",
		"codeception/module-webdriver": "^1.2",
		"ergebnis/composer-normalize": "^2.15",
		"mikey179/vfsstream": "^1.6",
		"phpstan/extension-installer": "^1.1",
		"phpstan/phpstan": "^0.12.99",
		"phpunit/phpunit": "^9.5",
		"rector/rector": "^0.11.58",
		"saschaegerer/phpstan-typo3": "^0.13.3",
		"symfony/yaml": "^4.4 || ^5.3",
		"symplify/phpstan-rules": "~9.3.0",
		"typo3/testing-framework": "^6.7"
	},
	"suggest": {
		"brotkrueml/typo3-jobrouter-data": "Connect JobRouter JobData tables with TYPO3",
		"brotkrueml/typo3-jobrouter-process": "Connect JobRouter processes with TYPO3"
	},
	"config": {
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.1.x-dev"
		},
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"extension-key": "jobrouter_connector",
			"web-dir": ".Build/Web"
		}
	},
	"autoload": {
		"psr-4": {
			"Brotkrueml\\JobRouterConnector\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Brotkrueml\\JobRouterConnector\\Tests\\": "Tests"
		}
	},
	"scripts": {
		"post-autoload-dump": ".Build/bin/captainhook install -f -s"
	},
	"support": {
		"issues": "https://github.com/brotkrueml/typo3-jobrouter-connector/issues",
		"source": "https://github.com/brotkrueml/typo3-jobrouter-connector",
		"docs": "https://typo3-jobrouter.readthedocs.io/projects/connector/"
	}
}
