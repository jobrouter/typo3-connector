{
	"name": "jobrouter/typo3-connector",
	"description": "Connect TYPO3 with the JobRouter速 digitalisation platform",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"keywords": [
		"TYPO3 CMS",
		"TYPO3 extension",
		"JobRouter"
	],
	"authors": [
		{
			"name": "Chris M端ller",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/jobrouter/typo3-connector/issues",
		"source": "https://github.com/jobrouter/typo3-connector",
		"docs": "https://docs.typo3.org/p/jobrouter/typo3-connector/main/en-us/"
	},
	"require": {
		"php": ">=8.1",
		"ext-sodium": "*",
		"jobrouter/rest-client": "^3.0@dev",
		"typo3/cms-backend": "^11.5.4 || ^12.4",
		"typo3/cms-core": "^11.5.4 || ^12.4",
		"typo3/cms-fluid": "^11.5.4 || ^12.4",
		"typo3/cms-frontend": "^11.5.4 || ^12.4"
	},
	"require-dev": {
		"brotkrueml/coding-standards": "~5.1.0",
		"codeception/codeception": "^5.0",
		"codeception/module-asserts": "^3.0",
		"codeception/module-webdriver": "^4.0",
		"ergebnis/composer-normalize": "~2.41.1",
		"mikey179/vfsstream": "^1.6.11",
		"phpstan/extension-installer": "^1.3",
		"phpstan/phpstan": "1.10.56",
		"phpunit/phpunit": "^10.5",
		"rector/rector": "0.19.5",
		"saschaegerer/phpstan-typo3": "^1.9",
		"symfony/yaml": "^5.4 || ^6.4 || ^7.0",
		"symplify/phpstan-rules": "^12.4",
		"tomasvotruba/cognitive-complexity": "^0.2.3",
		"typo3/testing-framework": "^7.0"
	},
	"suggest": {
		"jobrouter/typo3-data": "Connect JobRouter速 JobData tables with TYPO3",
		"jobrouter/typo3-process": "Connect JobRouter速 processes with TYPO3"
	},
	"autoload": {
		"psr-4": {
			"JobRouter\\AddOn\\Typo3Connector\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"JobRouter\\AddOn\\Typo3Connector\\Tests\\": "Tests"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"phpstan/extension-installer": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"branch-alias": {
			"dev-main": "3.0.x-dev"
		},
		"typo3/cms": {
			"app-dir": ".Build",
			"extension-key": "jobrouter_connector",
			"web-dir": ".Build/Web"
		}
	}
}
